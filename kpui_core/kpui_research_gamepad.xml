<GuiXml>
    <Controls>



	
		<Control name="Kela_TimerBar" hidden="true" virtual="true">
			<Dimensions x="280" y="12" />
			<Controls>
				<StatusBar name="$(parent)Status" hidden="false" >
					<OnInitialized>
						ZO_StatusBar_InitializeDefaultColors(self)
						self.gloss = self:GetNamedChild("Gloss")
					</OnInitialized>
					<AnchorFill/>
				</StatusBar>
				<Texture name="$(parent)TimerIcon" hidden="false" textureFile="EsoUI/Art/Miscellaneous/Gamepad/gp_icon_timer32.dds" tier="HIGH">
					<Dimensions x="26" y="26" />
					<Anchor point="RIGHT" relativeTo="$(parent)Status" relativePoint="RIGHT" offsetX="13" offsetY="0"/>
				</Texture>				
				<Label name="$(parent)Time" font="ZoFontGamepad18" hidden="false" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_SELECTED">
					<!-- <Anchor point="CENTER" /> -->
					<Anchor point="RIGHT" relativeTo="$(parent)TimerIcon" relativePoint="LEFT" offsetX="-1" offsetY="0"/>
				</Label>
			</Controls>
		</Control>

		<TopLevelControl name="Kela_GamepadSmithingTopLevelSkillInfo" inherits="ZO_GamepadSkillsFooter" hidden="true" />

